@model Register

<p>First Name: @Model.First_Name</p>
<p>Last Name: @Model.Last_Name</p>
<p>Email: @Model.Email</p>
<p>Phone number: @Model.Phone</p>
<p>DOB: @Model.DOB</p>

<form asp-controller="Home" asp-action="SignUp" method="post">
    <div class="form-group">
        <label for="FName">First Name: </label>
        <input type="text" asp-for="First_Name" class="form-control" />
    </div>
    <div class="form-group">
        <label for="LName">Last Name: </label>
        <input type="text" asp-for="Last_Name" class="form-control" />
    </div>
    <div class="form-group">
        <label for="Email">Email: </label>
        <input type="email" asp-for="Email" class="form-control" placeholder="abc@usf.edu"/>
    </div>
    <div class="form-group">
        <label for="Phone">Phone Number: </label>
        <input type="text" asp-for="Phone" class="form-control" />
    </div>
    <div class="form-group">
        <label for="DOB">Date of Birth: </label>
        <input type="date" asp-for="DOB" class="form-control" />
    </div>

    <button type="submit">Submit</button>
</form>

<a asp-controller="Home" asp-action="Index">
  Already have an account, click here to sign in!
</a>